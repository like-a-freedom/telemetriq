{
  "name": "sports-telemetry-overlay",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "fetch-ffmpeg-core": "mkdir -p public/vendor/ffmpeg && curl -fL https://cdn.jsdelivr.net/npm/@ffmpeg/core@latest/dist/esm/ffmpeg-core.js -o public/vendor/ffmpeg/ffmpeg-core.js && curl -fL https://cdn.jsdelivr.net/npm/@ffmpeg/core@latest/dist/esm/ffmpeg-core.wasm -o public/vendor/ffmpeg/ffmpeg-core.wasm",
    "benchmark:webgpu": "vitest run src/__tests__/webgpu-benchmark.test.ts",
    "benchmark:performance": "node scripts/benchmark.js"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@vueuse/core": "^14.2.1",
    "@webgpu/types": "^0.1.69",
    "mediabunny": "^1.34.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/dom-webcodecs": "^0.1.18",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^10.0.0",
    "happy-dom": "^20.6.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}